{
  "height": 800,
  "bg": "#fff2cc",

  "gutter": {
    "horizontal": 200,
    "vertical": 50
  },

  "grid": {
    "axis": "#000",
    "color": "#888",
    "dash": [2, 10],
    "step": 5,
    "intervals": 10
  },

  "candles": {
    "width": 8,
    "margin": 4,
    "white": {
      "body": "#fff",
      "border": "#222",
      "wick": "#222",
      "stroke": 1
    },
    "black": {
      "body": "#222",
      "border": "#222",
      "wick": "#222",
      "stroke": 1
    }
  }
}
